{
  "name": "js-building-blocks-hello-world",
  "description": "JS Building Blocks Hello World",
  "devDependencies": {
    "@hubspot/cli": "^4.0.1",
    "@hubspot/cms-dev-server": "^0.3.1",
    "@hubspot/prettier-plugin-hubl": "latest",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.10",
    "prettier": "^2.7.1"
  },
  "scripts": {
    "start": "hs-cms-dev-server hello-world-project/cms-assets",
    "lint:js": "eslint . --ext .js,.jsx",
    "prettier": "prettier . --check",
    "postinstall": "cd ./hello-world-project/cms-assets; $npm_execpath install",
    "watch:hubl": "hs watch hello-world-theme hello-world-theme",
    "upload:hubl": "hs upload hello-world-theme hello-world-theme",
    "deploy": "hs project upload hello-world-project",
    "deploy:local": "lhs project upload hello-world-project",
    "test": "cd ./hello-world-project/cms-assets; $npm_execpath run test"
  },
  "dependencies": {},
  "engines": {
    "node": ">=16.0.0"
  }
}
